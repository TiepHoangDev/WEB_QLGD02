  font-size: 35px;
  font-size: 2.33333rem;
  line-height: 1.42857; }

h2, .beta {
  font-size: 30px;
  font-size: 2rem;
  line-height: 1.66667; }

h3, .gamma {
  font-size: 28px;
  font-size: 1.86667rem;
  line-height: 1.78571; }

h4, .delta {
  font-size: 23px;
  font-size: 1.53333rem;
  line-height: 1.08696; }

h5, .feed-item__comments, .epsilon {
  font-size: 18px;
  font-size: 1.2rem;
  line-height: 1.38889; }

h6, .zeta {
  font-size: 15px;
  font-size: 1rem;
  line-height: 1.66667; }

/**
 * Heading groups and generic any-heading class.
 * To target any heading of any level simply apply a class of `.hN`, e.g.:
 *
   <hgroup>
       <h1 class=hN>inuit.css</h1>
       <h2 class=hN>Best. Framework. Ever!</h2>
   </hgroup>
 *
 */
