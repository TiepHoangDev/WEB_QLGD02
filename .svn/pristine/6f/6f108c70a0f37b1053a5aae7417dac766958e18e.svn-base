.module {
    word-wrap: break-word;
}
a {
    color: #07C;
    text-decoration: none;
    cursor: pointer;
}

.question-hyperlink:visited, .answer-hyperlink:visited, #hot-network-questions ul a:visited {
    color: #005999;
}
.module {
    margin-bottom: 1.5em;
}

#hot-network-questions h4 a {
    font-weight: inherit;
    font-size: inherit;
    width: inherit;
}

#hot-network-questions a {
    font-weight: normal;
    font-size: 12px;
    white-space: normal;
    width: 90%;
}

#hot-network-questions .favicon, #hot-network-questions a, #hot-network-questions img {
    display: inline-block;
    vertical-align: top;
}
#hot-network-questions .favicon, #hot-network-questions img {
    margin: 2px 6px 0 0;
}

#hot-network-questions .favicon, #hot-network-questions a, #hot-network-questions img {
    display: inline-block;
    vertical-align: top;
}

div.favicon {
    display: inline-block;
}

.favicon-workplace {
    background-position: 0 -6480px;
}

.favicon {
    width: 16px;
    height: 16px;
    background-color: transparent;
    background-repeat: no-repeat;
    background-image: url(../images/favicons-sprite16.png);
}

.question-hyperlink:visited, .answer-hyperlink:visited, #hot-network-questions ul a:visited {
    color: #005999;
}

#hot-network-questions a {
    font-weight: normal;
    font-size: 12px;
    white-space: normal;
    width: 90%;
}

#hot-network-questions .favicon, #hot-network-questions a, #hot-network-questions img {
    display: inline-block;
    vertical-align: top;
}

.layout__sidebar h4 {
    color: #242729;
    font-size: 18px;
    font-weight: 400;
}

.layout__sidebar h4, .sidebar h4 {
    margin-bottom: 1em;
}

.module h4 {
    font-size: 14px;
    color: #3b4045;
    font-weight: normal;
}
.show-votes .sidebar-linked .spacer>a:first-child .answer-votes, .show-votes .sidebar-related .spacer>a:first-child .answer-votes {
    padding: 3px 0;
    white-space: nowrap;
    width: 38px;
    text-align: center;
    box-sizing: border-box;
    height: auto;
    float: none;
    border-radius: 2px;
    font-size: 90%;
    background-color: #eff0f1;
    color: #3b4045;
    transform: translateY(-1px);
}

.answer-votes {
    cursor: pointer;
}

.answer-votes {
    color: #535a60;
    text-align: center;
    float: left;
    padding: 3px;
    min-width: 36px;
    min-height: 15px;
    text-decoration: none;
    border-radius: 2px;
}

.layout__sidebar .related a, .layout__sidebar .linked a {
    font-size: 13px;
}

.show-votes .sidebar-linked .spacer>a:first-child, .show-votes .sidebar-related .spacer>a:first-child {
    padding-right: 10px;
}

.related a {
    font-size: 12px;
    font-weight: normal;
}

